{
    "name": "@drtrt/inject-file-fragments",
    "version": "3.1.0",
    "description": "A simple NPM package that injects the contents of one or more files into another.",
    "keywords": [
        "inject",
        "include",
        "insert",
        "file",
        "fragments",
        "build",
        "CI",
        "automation",
        "script",
        "command-line",
        "cli",
        "template"
    ],
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "engines": {
        "node": ">=18"
    },
    "files": [
        "LICENSE",
        "README.md",
        "cli.js",
        "./dist/"
    ],
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.cjs",
            "import": "./dist/index.js"
        }
    },
    "bin": {
        "inject-file-fragments": "cli.js"
    },
    "scripts": {
        "prepack": "npm run build",
        "pretest": "rimraf testAssets/generated && mkdirp testAssets/generated",
        "test": "jest",
        "test:watch": "npm test -- --watch",
        "lint": "prettier --check . && eslint .",
        "format": "prettier --write . && eslint --fix .",
        "commitlint": "commitlint --edit",
        "semantic-release": "semantic-release --dry-run",
        "build": "tsup src/index.ts --format cjs,esm --dts --clean --sourcemap",
        "watch": "npm run build -- --watch src"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/drtrt-org/inject-file-fragments.git"
    },
    "author": "drtrt-org",
    "license": "MIT",
    "homepage": "https://github.com/drtrt-org/inject-file-fragments#readme",
    "devDependencies": {
        "@commitlint/cli": "19",
        "@commitlint/config-conventional": "19",
        "@eslint/js": "^9.9.0",
        "@semantic-release/changelog": "6",
        "@semantic-release/git": "10",
        "@semantic-release/github": "10",
        "@types/jest": "29",
        "eslint": "^9.9.0",
        "eslint-config-prettier": "9",
        "globals": "^15.9.0",
        "husky": "9",
        "jest": "29",
        "mkdirp": "3",
        "prettier": "3",
        "rimraf": "6",
        "semantic-release": "24",
        "ts-jest": "29",
        "tsup": "8",
        "typescript-eslint": "^8.2.0"
    },
    "overrides": {
        "conventional-changelog-conventionalcommits": ">= 8.0.0"
    }
}
