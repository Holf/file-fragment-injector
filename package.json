{
    "name": "@drtrt/inject-file-fragments",
    "version": "2.0.0",
    "description": "A simple NPM package that injects the contents of one or more files into another.",
    "keywords": [
        "inject",
        "include",
        "insert",
        "file",
        "fragments",
        "build",
        "CI",
        "automation",
        "script",
        "command-line",
        "cli",
        "template"
    ],
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "engines": {
        "node": ">=16.0.0 <20"
    },
    "files": [
        "LICENSE",
        "README.md",
        "cli.js",
        "src/index.js",
        "src/placeholderRegExp.js",
        "src/readFile.js",
        "src/index.mjs",
        "src/index.d.ts"
    ],
    "main": "./src/index.js",
    "module": "./src/index.mjs",
    "types": ".src/index.d.ts",
    "exports": {
        ".": {
            "require": "./src/index.js",
            "import": "./src/index.mjs"
        }
    },
    "bin": {
        "inject-file-fragments": "cli.js"
    },
    "scripts": {
        "pretest": "rimraf testAssets/generated && mkdirp testAssets/generated",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
        "test:watch": "npm test -- --watch",
        "format": "prettier --write .",
        "commitlint": "commitlint --edit",
        "semantic-release": "semantic-release --dry-run"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/drtrt-org/inject-file-fragments.git"
    },
    "author": "drtrt-org",
    "license": "MIT",
    "homepage": "https://github.com/drtrt-org/inject-file-fragments#readme",
    "devDependencies": {
        "@commitlint/cli": "^17.8.0",
        "@commitlint/config-conventional": "^17.8.0",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^9.2.1",
        "eslint": "^8.52.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-jest": "^27.4.3",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^5.0.1",
        "husky": "^8.0.3",
        "jest": "^29.6.1",
        "mkdirp": "^3.0.1",
        "prettier": "^3.0.3",
        "rimraf": "^5.0.5",
        "semantic-release": "^22.0.5"
    }
}
